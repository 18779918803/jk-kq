<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jk.db.mapper.DbDelayMapper">

    <sql id="Base_Column_List">
        is_pass,is_end,id,apply_type,db_id,db_source,db_lw_date,db_content_brief,db_ask_finaltime,db_level,lead_department,responsibility_department,apply_reason,up_time,unit_principal,unit_lead,manager_advice,president_advice,inserter,insert_time,updater,update_time,instance_id,create_by,create_time,update_by,update_time,apply_user,apply_time,pid
    </sql>

    <!-- 带参数查询督查督办延期（撤销）申请表列表 -->
    <select id="selectPageList" resultType="com.jk.db.entity.DbDelay">
        select
            <include refid="Base_Column_List"/>
        from db_delay where 1=1
            <if test="applyType !=null and applyType!=''">
                        and apply_type = #{applyType}
            </if>
            <if test="dbId !=null and dbId!=''">
                        and db_id = #{dbId}
            </if>

    </select>



</mapper>

